# OpenCarBox local environment template
# Copy to `.env.local` and fill values from your secret manager.
# Never commit real secrets.

NEXT_PUBLIC_SUPABASE_URL=https://<project-ref>.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=<SUPABASE_ANON_KEY>
SUPABASE_SERVICE_ROLE_KEY=<SUPABASE_SERVICE_ROLE_KEY>
SUPABASE_ACCESS_TOKEN=<SUPABASE_ACCESS_TOKEN>
MCP_SERVER_URL=https://mcp.supabase.com/mcp?project_ref=<project-ref>&features=docs,account,database,debugging,development,branching,functions,storage

DATABASE_URL=postgresql://postgres:<DB_PASSWORD>@db.<project-ref>.supabase.co:5432/postgres
DIRECT_URL=postgresql://postgres:<DB_PASSWORD>@db.<project-ref>.supabase.co:5432/postgres

NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Optional integrations
VERCEL_TOKEN=<VERCEL_TOKEN>
VERCEL_PROJECT_ID=<VERCEL_PROJECT_ID>
VERCEL_ORG_ID=<VERCEL_ORG_ID>
SNYK_TOKEN=<SNYK_TOKEN>
GOOGLE_GENERATIVE_AI_API_KEY=<GOOGLE_GENERATIVE_AI_API_KEY>
