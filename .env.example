# ===========================================
# OpenCarBox & Carvantooo - Environment Setup
# ===========================================

# SUPABASE
NEXT_PUBLIC_SUPABASE_URL=https://<project-ref>.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=<set-in-local-env>
SUPABASE_SERVICE_ROLE_KEY=<set-in-local-env>
SUPABASE_ACCESS_TOKEN=<set-in-local-env>
MCP_SERVER_URL=https://mcp.supabase.com/mcp?project_ref=<project-ref>&features=docs,account,database,debugging,development,branching,functions,storage

# DATABASE
DATABASE_URL=<set-in-local-env>
DIRECT_URL=<set-in-local-env>

# DEEPSEEK AI
DEEPSEEK_API_KEY=<set-in-local-env>
AGENT_MODEL=deepseek-chat

# GOOGLE AI
GOOGLE_GENERATIVE_AI_API_KEY=<set-in-local-env>

# PROJECT
PROJECT_ID=<project-ref>
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# STRIPE
STRIPE_SECRET_KEY=<set-in-local-env>
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=<set-in-local-env>
STRIPE_WEBHOOK_SECRET=<set-in-local-env>

# MEILISEARCH
MEILISEARCH_HOST=http://localhost:7700
MEILISEARCH_API_KEY=<set-in-local-env>

# RESEND
RESEND_API_KEY=<set-in-local-env>
RESEND_FROM_EMAIL=noreply@example.com

# OPTIONAL INTEGRATIONS
TECDOC_API_KEY=<set-in-local-env>
TECDOC_API_URL=<set-in-local-env>
GITHUB_TOKEN=<set-in-local-env>
GITHUB_REPOSITORY=owner/repo
BRAVE_API_KEY=<set-in-local-env>
QDRANT_URL=<set-in-local-env>
QDRANT_API_KEY=<set-in-local-env>
QDRANT_CLUSTER_ID=<set-in-local-env>
QDRANT_COLLECTION=nexify_neural_memory
API_KEY=<set-in-local-env>
