{
  "name": "opencarbox-carvantooo",
  "version": "1.0.0",
  "description": "OpenCarBox & Carvantooo - Premium Automotive Multisite Platform",
  "private": true,
  "scripts": {
    "postinstall": "node -e \"const fs = require('fs'); !fs.existsSync('.env.local') && process.env.DATABASE_URL === undefined && process.env.CI && !process.env.VERCEL ? console.log('⚠️  Skipping prisma generate in CI') : require('child_process').execSync('prisma generate', {stdio: 'inherit'})\" || prisma generate || true",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "secret:scan": "bash scripts/secret-scan.sh",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:rls:check": "psql $DATABASE_URL -f supabase/tests/rls_smoke.sql",
    "supabase:bootstrap": "bash scripts/supabase-bootstrap.sh",
    "prepare": "husky || true",
    "sync-rules": "tsx scripts/sync-docs-to-rules.ts",
    "quality-gate": "tsx scripts/quality-gate.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "oracle:test": "tsx scripts/test-oracle.ts",
    "oracle:sync": "tsx scripts/core/sync.ts",
    "oracle:sync-wiki": "tsx -e \"import { syncWiki } from './scripts/core/sync'; syncWiki()\"",
    "oracle:sync-docs": "tsx -e \"import { syncDocsToOracle } from './scripts/core/sync'; syncDocsToOracle()\"",
    "oracle:status": "tsx scripts/core/oracle-first.ts status",
    "oracle:before": "tsx scripts/core/oracle-first.ts before",
    "oracle:after": "tsx scripts/core/oracle-first.ts after",
    "oracle:queue": "tsx scripts/core/oracle-first.ts queue",
    "oracle:history": "tsx scripts/core/oracle-first.ts history",
    "oracle:end-session": "tsx scripts/core/oracle-first.ts end-session",
    "workflow:verify": "tsx -e \"import { verifyOnly } from './scripts/cline-workflows/recursive-intelligence'; verifyOnly()\"",
    "workflow:complete": "tsx scripts/cline-workflows/recursive-intelligence.ts complete",
    "error:search": "tsx scripts/cline-workflows/error-learning.ts search",
    "pre-change": "tsx scripts/cline-workflows/pre-change.ts",
    "cline:health": "tsx scripts/core/resilience.ts health",
    "cline:recover": "tsx scripts/core/resilience.ts recover",
    "cline:status": "tsx scripts/core/resilience.ts status",
    "cline:reset": "tsx scripts/core/resilience.ts reset",
    "cline:auto-restart": "tsx scripts/core/auto-restart.ts start",
    "cline:auto-status": "tsx scripts/core/auto-restart.ts status",
    "cline:auto-enable": "tsx scripts/core/auto-restart.ts enable",
    "cline:auto-disable": "tsx scripts/core/auto-restart.ts disable",
    "cline:auto-reset": "tsx scripts/core/auto-restart.ts reset",
    "watch:dev": "tsx scripts/core/watchdog.ts dev",
    "watch:script": "tsx scripts/core/watchdog.ts script",
    "watch:status": "tsx scripts/core/watchdog.ts status",
    "watch:reset": "tsx scripts/core/watchdog.ts reset",
    "autofix:problems": "tsx scripts/core/autonomous-problem-solver.ts",
    "report:plan-status": "tsx scripts/report-plan-status.ts",
    "report:weekly-status": "tsx scripts/report-weekly-status.ts",
    "system:start": "bash scripts/start-system.sh",
    "system:check": "bash scripts/start-system.sh check",
    "env:check": "tsx tools/check_env_schema.ts",
    "env:check:ci": "tsx tools/check_env_schema.ts --ci"
  },
  "packageManager": "pnpm@9.12.3",
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "@prisma/client": "^5.10.0",
    "@radix-ui/react-accordion": "^1.1.0",
    "@radix-ui/react-alert-dialog": "^1.0.0",
    "@radix-ui/react-avatar": "^1.0.0",
    "@radix-ui/react-checkbox": "^1.0.0",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "@radix-ui/react-label": "^2.0.0",
    "@radix-ui/react-popover": "^1.0.0",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.0.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.0.0",
    "@radix-ui/react-toast": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@stripe/stripe-js": "^3.0.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.97.0",
    "@tanstack/react-query": "^5.90.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.575.0",
    "meilisearch": "^0.38.0",
    "next": "^15.5.10",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.71.2",
    "resend": "^3.2.0",
    "sonner": "^2.0.7",
    "stripe": "^14.20.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0",
    "@storybook/addon-a11y": "^8.6.15",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/nextjs": "^8.0.0",
    "@storybook/react": "^8.6.15",
    "@types/node": "^20.19.30",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.3.0",
    "@vitest/ui": "^1.3.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.35",
    "glob": "^13.0.6",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "^5.10.0",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3",
    "vitest": "^1.3.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.17.0"
  }
}