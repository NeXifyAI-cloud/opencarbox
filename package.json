    "meilisearch": "^0.38.0",
    "next": "^14.2.0",
    "next-themes": "^0.4.6",
    "openai": "^6.15.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.51.0",
    "resend": "^3.2.0",
    "sonner": "^2.0.7",
    "stripe": "^14.20.0",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.0",
    "zustand": "^4.5.0"
